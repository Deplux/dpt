{
  "name": "depluxtools",
  "version": "1.0.4",
  "description": "dpt(Deplux tool)로 여러분의 호스팅을 손쉽게 가꾸어보세요",
  "main": "./dist/App.js",
  "scripts": {
    "build": "tsc",
    "dpt": "tsc && node ./dist/App.js",
    "dpt:all": "tsc && node ./dist/App.js --init --zip",
    "dpt:init": "tsc && node ./dist/App.js --init",
    "dpt:zip": "tsc && node ./dist/App.js --zip",
    "dpt:help": "tsc && node ./dist/App.js --help",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deplux/dpt.git"
  },
  "bin": {
    "dpt": "./App"
  },
  "author": "Deplux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deplux/dpt/issues"
  },
  "homepage": "https://github.com/Deplux/dpt#readme",
  "dependencies": {
    "@types/archiver": "^5.1.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.16",
    "@types/yargs": "^15.0.12",
    "archiver": "^5.1.0",
    "fs-extra": "^9.0.1",
    "yargs": "^16.2.0"
  }
}
